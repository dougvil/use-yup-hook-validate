{
  "name": "use-yup-hook-validate",
  "version": "2.1.0",
  "description": "A hook for fast form field validation without headache",
  "author": {
    "name": "Douglas Viliano",
    "email": "dougvil@msn.com",
    "url": "https://github.com/dougvil"
  },
  "keywords": [
    "react",
    "hooks",
    "react-hooks",
    "yup"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write .",
    "bump": "npm version",
    "example": "cd example && yarn install && yarn package",
    "prepublishOnly": "npm run clean && npm run build",
    "watch": "tsc -w",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dougvil/use-yup-hook-validate"
  },
  "bugs": {
    "url": "https://github.com/dougvil/use-yup-hook-validate/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "typescript": "5.5.4",
    "rimraf": "5.0.5",
    "@types/react": "^18.2.23",
    "@types/lodash": "^4.14.202",
    "react": "^18.2.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0"
  }
}
